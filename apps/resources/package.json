{
  "name": "resources-co",
  "version": "1.0.0-canary.17",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "concurrently -k -n app,api,embed yarn:dev:app yarn:dev:api-proxy yarn:dev:embed-proxy",
    "dev:browser": "NEXT_PUBLIC_BROWSER_STORAGE=true concurrently -k -n app,api,embed yarn:dev:app yarn:dev:api-proxy yarn:dev:embed-proxy",
    "dev:app": "NEXT_PUBLIC_API_BASE=http://localhost:3334/api next dev -p 3333",
    "dev:api-proxy": "PORT=3333 PROXY_PORT=3334 ts-node --compiler-options '{\"module\": \"commonjs\"}' scripts/proxy.ts",
    "dev:embed-proxy": "PORT=3333 PROXY_PORT=3335 ts-node --compiler-options '{\"module\": \"commonjs\"}' scripts/proxy.ts",
    "build": "cross-env \"NEXT_PUBLIC_EMBED_BASE_URL_DEFAULT=https://$VERCEL_URL\" next build",
    "start": "next start",
    "export": "next export",
    "format": "prettier --write \"**/*.js\"",
    "electron-renderer:build": "NEXT_PUBLIC_ELECTRON=true next build",
    "electron-renderer:export": "NEXT_PUBLIC_ELECTRON=true next export -o renderer",
    "desktop:start": "cd packages/desktop && yarn install && yarn start",
    "desktop:make": "cd packages/desktop && yarn install && yarn make"
  },
  "dependencies": {},
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.5.2",
    "@types/chroma-js": "^2.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.5",
    "@types/react": "^16.9.35",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.2.0",
    "http-proxy": "^1.18.1",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.5"
  }
}
