    "immer": "^7.0.9",
    "insert-text-at-cursor": "^0.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "j2ref": "^1.0.0",
    "json-ptr": "^1.3.2",
    "lodash": "^4.17.20",
    "nanoevents": "^5.1.8",
    "nanoid": "^3.1.12",
