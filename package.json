{
  "name": "@resources/resources-workspace",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx start",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev": "concurrently -k -n app,api,embed npm:dev:app npm:dev:api-proxy npm:dev:embed-proxy",
    "dev:browser": "NEXT_PUBLIC_BROWSER_STORAGE=true concurrently -k -n app,api,embed npm:dev:app npm:dev:api-proxy npm:dev:embed-proxy",
    "dev:app": "NEXT_PUBLIC_API_BASE=http://localhost:3334/api nx serve resources --port=3333",
    "dev:api-proxy": "PORT=3333 PROXY_PORT=3334 ts-node --compiler-options '{\"module\": \"commonjs\"}' packages/resources/scripts/proxy.ts",
    "dev:embed-proxy": "PORT=3333 PROXY_PORT=3335 ts-node --compiler-options '{\"module\": \"commonjs\"}' packages/resources/scripts/proxy.ts",
    "electron-renderer:build": "NEXT_PUBLIC_ELECTRON=true nx build resources",
    "electron-renderer:export": "NEXT_PUBLIC_ELECTRON=true nx export resources",
    "desktop:renderer:import": "rm -rf ./packages/desktop/renderer && cp -r dist/packages/resources/exported ./packages/desktop/renderer",
    "desktop:main:import": "rm -rf ./packages/desktop/src/api ./packages/desktop/src/vtv-model && cp -r ./packages/resources/api ./packages/desktop/src/api && cp -r ./packages/resources/vtv-model ./packages/desktop/src/vtv-model",
    "desktop:mainjs:build": "tsc packages/desktop/src/main.ts --outDir packages/desktop/main --target es2020 --moduleResolution node --module commonjs",
    "desktop:preload:build": "tsc packages/desktop/src/preload.ts --outFile packages/desktop/main/preload.js --target es2020 --moduleResolution node",
    "desktop:main:build": "rm -rf ./packages/desktop/main && npm run desktop:mainjs:build && npm run desktop:preload:build",
    "desktop:electron:start": "cd ./packages/desktop && npm run start",
    "desktop:electron:make": "cd ./packages/desktop && npm run make",
    "desktop:build-all": "npm run electron-renderer:build && npm run electron-renderer:export && npm run desktop:renderer:import && npm run desktop:main:import && npm run desktop:main:build",
    "desktop:start": "npm run desktop:build-all && npm run desktop:electron:start",
    "desktop:make": "npm run desktop:build-all && npm run desktop:electron:make",
    "vtv:build": "nx build vtv && cp -r packages/vtv/src/css dist/packages/vtv/css"
  },
  "private": true,
  "dependencies": {
    "@braintree/sanitize-url": "^4.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@popperjs/core": "^2.4.0",
