{
  "name": "@resources/desktop",
  "version": "0.1.0",
  "description": "Resources.co Desktop App",
  "main": "./main/index.js",
  "type": "commonjs",
  "scripts": {
    "make": "electron-forge make",
    "package": "electron-forge package",
    "start": "yarn main:import && yarn main:build && yarn renderer:build && yarn renderer:import && yarn electron:start",
    "main:import": "rm -rf ./src/api ./src/vtv-model && cp -r ../../api ./src/api && cp -r ../../vtv-model ./src/vtv-model",
    "main:build": "tsc",
    "main:watch": "tsc -w",
    "renderer:build": "cd ../.. && yarn electron-renderer:build && yarn electron-renderer:export",
    "renderer:import": "rm -rf ./renderer && cp -r ../../renderer ./renderer",
    "electron:start": "electron-forge start"
  },
  "keywords": [],
  "author": {
    "name": "Benjamin Atkin",
    "email": "ben@benatkin.com"
  },
  "license": "MIT",
  "config": {
    "forge": "./resources.forge.config.js"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "immer": "^7.0.8",
    "isomorphic-unfetch": "^3.0.0",
    "j2ref": "^1.0.0",
    "json-ptr": "^1.3.2",
    "lodash": "^4.17.19",
    "nanoevents": "^5.1.8",
    "nanoid": "^3.1.12",
    "path-to-regexp": "^6.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@types/node": "14.6.2",
    "concurrently": "^5.1.0",
    "electron": "^10.1.1",
    "electron-reloader": "^1.0.1",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  }
}
