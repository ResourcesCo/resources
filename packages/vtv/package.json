{
  "name": "vtv",
  "version": "1.0.0-canary.0",
  "description": "versatile tree view",
  "main": "./build/index.js",
  "module": "./build/esm/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js ./src --out-dir ./build --ignore \"**/*.test.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js ./src --out-dir ./build/esm --ignore \"**/*.test.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js ./src --out-dir ./build/es --ignore \"**/*.test.js\"",
    "release": "npm run build && npm publish build --tag latest",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "clsx": "^1.0.4",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-autosize-textarea": "^7.0.0",
    "react-is": "^16.8.0",
    "react-popper": "^1.3.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "cross-env": "^6.0.3",
    "next": "^9.2.0"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
